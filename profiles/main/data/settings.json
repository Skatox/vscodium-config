{
  "editor.accessibilitySupport": "off",
  "editor.fontFamily": "'JetBrains Mono','Fira Sans Mono', 'monospace', monospace",
  "editor.fontSize": 15,
  "editor.hideCursorInOverviewRuler": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.overviewRulerBorder": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "keyword.other.dotenv",
        "settings": {
          "foreground": "#FF000000"
        }
      }
    ]
  },
  "files.associations": {
    "*.module": "php",
    ".env*": "dotenv"
  },
  "github.copilot.enable": {
    "*": true,
    "markdown": false,
    "plaintext": false,
    "scminput": false
  },
  "npm.enableRunFromFolder": true,
  "npm.scriptExplorerAction": "run",
  "php-cs-fixer.executablePath": "/opt/homebrew/bin/php-cs-fixer",
  "search.showLineNumbers": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "vim.enableNeovim": true,
  "vim.leader": " ",
  "vim.useCtrlKeys": true,
  "vscode-neovim.mouseSelectionStartVisualMode": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.sideBar.location": "right",
  "workbench.colorTheme": "One Monokai (Light)",
  "explorer.autoRevealExclude": {
    "**/node_modules": false
  },
  "scm.inputFontSize": 15,
  "debug.console.fontSize": 14,
  "markdown.preview.fontSize": 15,
  "terminal.integrated.fontSize": 14,
  "chat.editor.fontSize": 14,
  "jest.autoRevealOutput": "on-exec-error",
  "jest.autoRun": {},
  "vim.highlightedyank.enable": true,
  "window.zoomLevel": 2,
  "diffEditor.ignoreTrimWhitespace": false,
  "vim.autoSwitchInputMethod.enable": true,
  "git.suggestSmartCommit": false,
  "vim.easymotion": true,
  "vim.neovimPath": "/opt/homebrew/bin/nvim",
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "vim.neovimConfigPath": "~/.config/nvim/init.lua",
  "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/init.lua",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  }
}
